<Window x:Class="UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="GetDataSync" Content="GetDataSync" HorizontalAlignment="Left" Margin="80,50,0,0" VerticalAlignment="Top" Height="50" Width="160" Click="OnExecuteSyncClicked"/>
        <Button x:Name="GetDataAsync" Content="GetDataAsync"
                HorizontalAlignment="Center" VerticalAlignment="Top" Height="50" Width="160" Margin="0,50,0,0" Click="OnExecuteAsyncClicked"/>
        <Button x:Name="GetDataAsyncParallel" Content="GetDataAsyncParallel"
                HorizontalAlignment="Left" VerticalAlignment="Top" Height="50" Width="160" Margin="560,50,0,0" Click="OnExecuteAsyncParallelClicked"/>
        <TextBlock x:Name="ResultBlock" HorizontalAlignment="Center" Height="280" Margin="0,120,0,0" VerticalAlignment="Top" Width="640"/>
    </Grid>
</Window>
